{
    "name": "@foody/source",
    "version": "1.1.2",
    "private": true,
    "license": "MIT",
    "workspaces": [
        "web",
        "web-e2e",
        "api",
        "api-e2e"
    ],
    "scripts": {
        "build": "nx run-many --target=build --all",
        "build:api": "nx build @foody/api",
        "build:web": "nx build @foody/web",
        "cache:clear": "cd api && npm run clear-cache",
        "db:format": "cd api && npx prisma format",
        "db:generate": "cd api && npx prisma generate",
        "db:migrate": "cd api && npx prisma migrate dev",
        "db:reset": "cd api && npx prisma migrate reset --force",
        "db:seed": "cd api && npx prisma db seed",
        "db:setup": "npm run db:reset && npm run db:migrate && npm run db:seed",
        "db:studio": "cd api && npx prisma studio",
        "dev": "nx run-many --target=dev --parallel",
        "dev:api": "nx dev @foody/api",
        "dev:web": "nx dev @foody/web",
        "e2e": "nx run-many --target=e2e --all",
        "e2e:api": "nx e2e @foody/api-e2e",
        "e2e:web": "nx e2e @foody/web-e2e",
        "format": "nx format:write",
        "format:check": "nx format:check",
        "lint": "nx run-many --target=lint --all",
        "lint:api": "nx lint @foody/api",
        "lint:web": "nx lint @foody/web",
        "nx": "nx",
        "prepare": "husky",
        "test": "nx run-many --target=test --all",
        "test:api": "nx test @foody/api",
        "test:web": "nx test @foody/web",
        "validate": "npm run format:check && npm run lint && npm run typecheck && npm run test -- --watchAll=false --passWithNoTests",
        "typecheck": "nx run-many --target=typecheck --all",
        "typecheck:api": "nx typecheck @foody/api",
        "typecheck:api-e2e": "nx typecheck @foody/api-e2e",
        "typecheck:web": "nx typecheck @foody/web",
        "typecheck:web-e2e": "nx typecheck @foody/web-e2e"
    },
    "lint-staged": {
        "*.{js,ts,tsx,jsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "@tailwindcss/postcss": "^4.1.17",
        "axios": "^1.13.5",
        "express": "^4.21.2",
        "next": "^15.5.12",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.2.0",
        "@eslint/compat": "^1.1.1",
        "@eslint/eslintrc": "^2.1.1",
        "@eslint/js": "^9.8.0",
        "@next/eslint-plugin-next": "^15.2.4",
        "@nx/devkit": "22.0.3",
        "@nx/esbuild": "22.0.3",
        "@nx/eslint": "22.0.3",
        "@nx/eslint-plugin": "22.0.3",
        "@nx/jest": "22.0.3",
        "@nx/js": "22.0.3",
        "@nx/next": "22.0.3",
        "@nx/node": "^22.0.3",
        "@nx/playwright": "22.0.3",
        "@nx/workspace": "22.0.3",
        "@playwright/test": "^1.57.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^12.0.2",
        "@semantic-release/npm": "^13.1.2",
        "@semantic-release/release-notes-generator": "^14.1.0",
        "@swc-node/register": "~1.9.1",
        "@swc/cli": "~0.6.0",
        "@swc/core": "~1.5.7",
        "@swc/helpers": "~0.5.11",
        "@swc/jest": "~0.2.38",
        "@testing-library/dom": "10.4.1",
        "@testing-library/react": "16.3.0",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/node": "20.19.9",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "autoprefixer": "10.4.13",
        "babel-jest": "^30.0.2",
        "dotenv": "^17.2.3",
        "esbuild": "^0.27.0",
        "eslint": "^9.8.0",
        "eslint-config-next": "^15.2.4",
        "eslint-config-prettier": "^10.0.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-jsx-a11y": "6.10.1",
        "eslint-plugin-playwright": "^1.6.2",
        "eslint-plugin-react": "7.35.0",
        "eslint-plugin-react-hooks": "7.0.1",
        "husky": "^9.1.7",
        "jest": "^30.0.2",
        "jest-environment-jsdom": "^30.0.2",
        "jest-environment-node": "^30.0.2",
        "jest-util": "^30.0.2",
        "lint-staged": "^16.2.7",
        "nx": "22.0.3",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "semantic-release": "^25.0.2",
        "tailwindcss": "^4.1.17",
        "ts-jest": "^29.4.0",
        "ts-node": "10.9.1",
        "tslib": "^2.3.0",
        "typescript": "~5.9.2",
        "typescript-eslint": "^8.40.0"
    }
}

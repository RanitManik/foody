{
    "id": "foody-api-environment",
    "name": "Foody API Environment",
    "values": [
        {
            "key": "baseUrl",
            "value": "http://localhost:4000",
            "description": "Base URL for the Foody API server",
            "enabled": true
        },
        {
            "key": "graphql",
            "value": "{{baseUrl}}/graphql",
            "description": "GraphQL endpoint URL",
            "enabled": true
        },
        {
            "key": "auth_token",
            "value": "your-jwt-token-here",
            "description": "JWT authentication token obtained after login/register",
            "enabled": true
        }
    ],
    "_postman_variable_scope": "environment"
}
